{
    "swagger": "2.0",
    "info": {
      "version": "1.0.0",
      "title": "DMS Backend API",
      "description": "API Documentation for the DMS Backend"
    },
    "host": "localhost:8080",
    "basePath": "/DMSSystem",
    "schemes": [
      "https"
    ],
    "paths": {
      "/users": {
        "post": {
          "summary": "Create a new user",
          "description": "Create a new user in the system",
          "parameters": [
            {
              "name": "body",
              "in": "body",
              "required": true,
              "schema": {
                "$ref": "#/definitions/User"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "User created successfully"
            },
            "400": {
              "description": "Invalid input"
            }
          }
        }
      }
    },
    "definitions": {
      "User": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "user@example.com"
          },
          "password": {
            "type": "string",
            "example": "password"
          }
        }
      }
    }
  }
  